<div class="profile-wrapper">

<h1 class = "middle"> 
  <%= current_user.first_name.titleize%>
  <div> Your Profile</div>
</h1>

  <h2> Restaurants You Reviewed </h2>

</div>
<div class="row">
    <% if @my_review_resturant.blank? %>
        <h3>Could not find a restaurant</h3>
    <%else%>
        <% @my_review_resturant.each do |resturant| %>
            <div class="col-md-3">
              <a class="overlay" href="resturant" title="resturant.name">
                <span class="more"></span>
                <% if resturant.image.url.present? %>
                    <%= link_to image_tag(resturant.image_url.to_s, :size=>"370x210"), resturant %>
                <% else %>
                    <%= link_to image_tag("http://placehold.it/370x210"), resturant %>
                <% end %>
              </a>
              <span class="prop-tag"></span>
              <div class="prop-info">
                <h3 class="prop-title"><%= link_to resturant.name, resturant %></h3>
              </div><!-- prop-info ending point -->
            </div><!-- holder ending point -->
        <% end %>
    <% end %>
  </div><!-- row ending point -->

<h2> Your Favorites </h2>

</div>
<div class="row">
    <% if @my_favorites.blank? %>
        <h3>Could not find a restaurant</h3>
    <%else%>
        <% @my_favorites.each do |resturant| %>
            <div class="col-md-3">
              <a class="overlay" href="resturant" title="resturant.name">
                <span class="more"></span>
                <% if resturant.image.url.present? %>
                    <%= link_to image_tag(resturant.image_url.to_s, :size=>"370x210"), resturant %>
                <% else %>
                    <%= link_to image_tag("http://placehold.it/370x210"), resturant %>
                <% end %>
              </a>
              <span class="prop-tag"></span>
              <div class="prop-info">
                <h3 class="prop-title"><%= link_to resturant.name, resturant %></h3>
              </div><!-- prop-info ending point -->
            </div><!-- holder ending point -->
        <% end %>
    <% end %>
  </div><!-- row ending point -->


</div><!-- container ending point-->
